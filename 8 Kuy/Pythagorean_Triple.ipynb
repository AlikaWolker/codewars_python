      "source": [
        "def pythagorean_triple(integers):\n",
        "    integers.sort()\n",
        "    a = integers[0]\n",
        "    b = integers[1]\n",
        "    c = integers[2]\n",
        "    if (c^2 != (a^2+b^2)):\n",
        "        return False \n",
        "    else:\n",
        "        return True"
      ]
    }
  ]
